{
  "compilerOptions": {
    "target": "es2016",
    "module": "commonjs",
    "esModuleInterop": true,
    "forceConsistentCasingInFileNames": true,
    "strict": true,
    "skipLibCheck": true,
    "outDir": "dist",
    "resolveJsonModule": true,
    "moduleResolution": "node"
  },
  "include": ["server", "shared"],
  "exclude": ["node_modules", "client", "dist"]
}
